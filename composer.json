{
    "name": "cyril-verloop/doctrine-entities",
    "description": "Some default doctrine entity/fields requiring PHP 8.1+ and Doctrine 2.13+.",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "CyrilVerloop\\DoctrineEntities\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CyrilVerloop\\DoctrineEntities\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": ">=8.1",
        "doctrine/orm": "^2.15"
    },
    "authors": [
        {
            "name": "Cyril VERLOOP",
            "email": "cyril.verloop@webqard.fr"
        }
    ],
    "config": {
        "allow-plugins": {
            "composer/package-versions-deprecated": true
        }
    }
}
